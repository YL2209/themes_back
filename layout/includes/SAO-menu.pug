#SAO-back
  #SAO-menu
    #SAO-menu-content
      .utils-list
        if theme.SAO_Utils.util_list
          each util in theme.SAO_Utils.util_list
            .utils-list-item
              if util.panel
                .user-panel(style=`top:`+ util.panel.top + `;`)
                  .user-panel-name!= util.panel.title
                  .user-panel-img
                    - var userImg = util.panel.img ? url_for(util.panel.img) : 'https://npm.elemecdn.com/akilar-candyassets/image/info.png'
                    img(src=userImg)
                  .user-panel-properties!=`${util.panel.prop}`
              if util.link
                i(class=util.icon onclick=`clickAudio();setTimeout(function(){SAOclose();linkStart('` +url_for(util.link)+ `')},500);`)
              else if util.action
                i(class=util.icon onclick=`clickAudio();setTimeout(function(){SAOclose();` + util.action + `},500);`)
              else
                i(class=util.icon onclick="panelAudio();UtilsClick()")
                if util.menu_list
                  - var menuTop = util.menutop ? util.menutop : -((util.menu_list.length)*40/2+43)
                  .menu-list(style=`top:` + menuTop + `px;`)
                    each item in util.menu_list
                      .menu-list-item
                        if item.link
                          i(class=item.icon onclick=`clickAudio();setTimeout(function(){SAOclose();linkStart('` +url_for(item.link)+ `')},500);`)= item.name
                        else if item.action
                          i(class=item.icon onclick=`clickAudio();setTimeout(function(){SAOclose();` + item.action + `},500);`)= item.name
                        else
                          i(class=item.icon onclick='panelAudio();MenusClick()')= item.name
                        if item.child_list
                          - var childTop = item.childtop ? item.childtop : -((item.child_list.length)*40/2+20)
                          .menu-child(style=`top:` + childTop + `px;`)
                            each child in item.child_list
                              .menu-list-child
                                if child.link
                                  i(class=child.icon onclick=`clickAudio();setTimeout(function(){SAOclose();linkStart('` +url_for(child.link)+ `')},500);`)= child.name
                                else if child.action
                                  i(class=child.icon onclick=`clickAudio();setTimeout(function(){SAOclose();` + child.action + `},500);`)= child.name
                                else
                                  i(class=child.icon)= child.name
        if theme.SAO_Utils.Instructions ? theme.SAO_Utils.Instructions : true
          .utils-list-item
            .user-panel
              .user-panel-name 说明书
              .user-panel-img
                img(src='/img/favicon.png')
              .user-panel-properties
                h4 欢迎使用SAO_Utils_Web 2.0
                p 点按选项可以持续显示下级菜单。您可以通过按住<kbd>ctrl</kbd>+<kbd>右键</kbd>来恢复使用原生右键菜单，更多内容可点击下方链接访问源教程。
                center
                  a(href='https://akilar.top/posts/fd243d7/') ©Akilarの糖果屋
            i.fa.fa-cog.fa-spin(onclick="panelAudio();UtilsClick()")
            .menu-list(style="top:-163px;")
              .menu-list-item
                i.fas.fa-adjust(onclick="clickAudio();openDarkMode()" title="浅色和深色模式转换") 模式切换
              .menu-list-item
                i.fas.fa-images(onclick="panelAudio();MenusClick()" title="背景图片切换") 背景切换
                .menu-child(style="top:-75px;")
                  .menu-list-child
                    i.fas.fa-images(onclick="imgeAudio();openImage()" title="动漫背景图片切换") 动漫背景
                  .menu-list-child
                    i.fas.fa-clone(onclick="imgeAudio();openImageGif()" title="动态背景图片切换") 动态背景
                  .menu-list-child
                    i.fas.fa-images(onclick="imgeAudio();openImageApi()" title="简约背景图片切换") 简约背景
              .menu-list-item
                i.fas.fa-heartbeat(onclick="clickAudio();openlive2d()" title="看板娘隐显转换") 看板娘开关
              .menu-list-item
                i.fas.fa-book-open(onclick="panelAudio();MenusClick()" title="文章相关设置") 文章设置
                .menu-child(style="top:-75px;")
                  .menu-list-child
                    i.fas.fa-arrows-alt-h(onclick="clickAudio();openhideAsideBtn()" title="单栏和双栏切换") 排版切换
                  .menu-list-child
                    i.fas.fa-book-open(onclick="clickAudio();openReadMode()" title="阅读模式转换") 阅读模式
                  .menu-list-child
                    i.fas.fa-comments(onclick="clickAudio();ToComment()" title="直达评论") 评论
              .menu-list-item
                i.fas.fa-arrow-up(onclick="clickAudio();openToTop()" title="直达顶部") 返回顶部
              .menu-list-item
                i.fa.fa-sign-out-alt(onclick="alertAudio();openLogout()" title="退出浏览窗口") 退出
        .utils-list-item
          i.fa.fa-power-off(onclick="alertAudio();SAOKeepOff();SnackbarIMGES('你已关闭SAO右键菜单', 2000)" title="关闭SAO右键菜单")
if theme.SAO_Utils.Instructions ? theme.SAO_Utils.Instructions : true
  #SAO-logout
    .logout-title Alert
    .logout-alert 是否确认退出?
    .logout-button
      span.logout-confirm
        button.far.fa-circle(type="button" name="confirm" onclick="LogoffAudio();confirmLogout()")
      span.logout-cancel
        button.fa.fa-times(type="button" name="cancel" onclick="LogonAudio();cancelLogout()")
if theme.SAO_Utils.music.enable
  - var Launcher = theme.SAO_Utils.music.Launcher ? url_for(theme.SAO_Utils.music.Launcher) : 'https://npm.elemecdn.com/akilar-candyassets/audio/Launcher.mp3'
  - var Click = theme.SAO_Utils.music.Click ? url_for(theme.SAO_Utils.music.Click) : 'https://npm.elemecdn.com/akilar-candyassets/audio/Click.mp3'
  - var Panel = theme.SAO_Utils.music.Panel ? url_for(theme.SAO_Utils.music.Panel) : 'https://npm.elemecdn.com/akilar-candyassets/audio/Panel.mp3'
  - var Alert = theme.SAO_Utils.music.Alert ? url_for(theme.SAO_Utils.music.Alert) : 'https://npm.elemecdn.com/akilar-candyassets/audio/Alert.mp3'
    - var YUImg = theme.SAO_Utils.music.YUImg ? url_for(theme.SAO_Utils.music.YUImg) : 'https://cdn.jsdelivr.net/gh/XXKDB/img_cdn/cursor/img.mp3'
  - var Logoff = theme.SAO_Utils.music.Logoff ? url_for(theme.SAO_Utils.music.Logoff) : 'https://cdn.jsdelivr.net/gh/XXKDB/img_cdn/cursor/Logoff.mp3'
  - var Logon = theme.SAO_Utils.music.Logon ? url_for(theme.SAO_Utils.music.Logon) : 'https://cdn.jsdelivr.net/gh/XXKDB/img_cdn/cursor/Logon.mp3'
  audio#SAOlauncher(src=Launcher)
  audio#SAOClick(src=Click)
  audio#SAOPanel(src=Panel)
  audio#SAOAlert(src=Alert)
  audio#SAOImge(src=YUImg)
  audio#SAOLogoff(src=Logoff)
  audio#SAOLogon(src=Logon)
script(async src=url_for(theme.SAO_Utils.CDN))